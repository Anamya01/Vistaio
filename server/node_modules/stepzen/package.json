{
  "name": "stepzen",
  "description": "The StepZen CLI",
  "version": "0.36.1",
  "license": "MIT",
  "author": "Darren Waddell <darren@stepzen.com>",
  "contributors": [
    "Lucia Cerchie <lucia@stepzen.com"
  ],
  "homepage": "https://stepzen.com",
  "bin": {
    "stepzen": "./bin/run"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.0",
    "npm": ">=6.14"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "graphql"
  ],
  "dependencies": {
    "@oclif/core": "2.0.11",
    "@oclif/plugin-help": "5.2.4",
    "@oclif/plugin-version": "1.2.1",
    "@soluble/dsn-parser": "^1.7.5",
    "@stepzen/fetch": "0.36.1",
    "@stepzen/graphiql-proxy": "0.36.1",
    "@stepzen/sdk": "0.36.1",
    "@stepzen/transpiler": "0.36.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "compare-versions": "^5.0.3",
    "debug": "^4.3.4",
    "detect-port": "^1.5.1",
    "dotenv": "^16.0.3",
    "fs-extra": "^9.1.0",
    "glob": "^7.2.3",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.11.2",
    "inquirer": "^8.2.5",
    "lodash": "^4.17.21",
    "node-abort-controller": "^3.1.1",
    "prettier": "^2.8.3",
    "pretty-ms": "^7.0.1",
    "puka": "^1.0.1",
    "shelljs": "^0.8.5",
    "string-argv": "^0.3.1",
    "uuid": "^8.3.2",
    "ws": "^8.11.0",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@oclif/test": "2.3.5",
    "@types/chai": "^4.3.10",
    "@types/debug": "^4.1.7",
    "@types/detect-port": "^1.3.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.5",
    "@types/license-checker": "^25.0.1",
    "@types/mocha": "^10.0.1",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.18.36",
    "@types/node-fetch": "^2.6.2",
    "@types/prettier": "^2.7.2",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.11",
    "@types/sinon-chai": "3.2.8",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "chai": "^4.3.10",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "7.11.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "3.3.1",
    "license-checker": "^25.0.1",
    "mocha": "^10.2.0",
    "mock-fs": "^5.2.0",
    "nock": "^13.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "oclif": "3.6.3",
    "semver": "^7.5.4",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "strip-ansi": "^6.0.1",
    "ts-node": "^10.8.1"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stepzen",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/version"
      ],
      "prerun": [
        "./lib/hooks/prerun/check-upgrade",
        "./lib/hooks/prerun/ensure-config-file",
        "./lib/hooks/prerun/ensure-permissions"
      ]
    },
    "topicSeparator": " ",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "scripts": {
    "dev:login": "nodemon -e ts bin/run login",
    "dev:start": "nodemon -e ts bin/run start --dir ~/Downloads/hello-stepzen",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ignore-path .prettierignore",
    "prepack": "oclif manifest && oclif readme",
    "test": "npm run build && cross-env STEPZEN_INTERACTIVE=true STEPZEN_SERVER_URL=https://test.steprz.net nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:single:example": "npm run test:single -- test/src/shared/curl-parser.test.ts",
    "test:single": "npm run build && cross-env STEPZEN_INTERACTIVE=true STEPZEN_SERVER_URL=https://test.steprz.net mocha test/_global.test.ts",
    "clean": "rm -rf lib tsconfig.tsbuildinfo",
    "build": "../../node_modules/.bin/tsc -b",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "overrides": {
    "@oclif/core": {
      "password-prompt": {
        "--comment--": "workaround for CVE-2022-25883",
        "cross-spawn": "^7.0.0"
      }
    }
  },
  "gitHead": "d9968fc5de73b58186c283034ed2b7b63d81f010"
}
