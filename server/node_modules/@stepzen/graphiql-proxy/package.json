{
  "name": "@stepzen/graphiql-proxy",
  "version": "0.36.1",
  "author": "Darren Waddell <darren@stepzen.com>",
  "license": "MIT",
  "homepage": "https://stepzen.com",
  "files": [
    "/src/server",
    "/src/ui/build"
  ],
  "engines": {
    "node": ">=14.0.1",
    "npm": ">=6.14"
  },
  "main": "src/server",
  "scripts": {
    "dev:server": "nodemon src/dev --watch src/dev --watch src/server",
    "dev:ui": "cd src/ui && REACT_APP_IS_DEVELOPMENT=1 npm start",
    "prepare": "cd src/ui && npm install --legacy-peer-deps",
    "build": "cd src/ui && npm run build",
    "start": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "prettier . --check"
  },
  "dependencies": {
    "@stepzen/fetch": "0.36.1",
    "chalk": "^4.1.2",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "express": "^4.19.2",
    "express-promise-router": "^4.1.1",
    "open": "^8.4.0",
    "prettier": "^2.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.2.0",
    "@types/license-checker": "^25.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.18.36",
    "@types/sinon-chai": "3.2.8",
    "chai": "^4.3.10",
    "concurrently": "^6.0.2",
    "dotenv": "^16.0.3",
    "fancy-test": "^1.4.10",
    "license-checker": "^25.0.1",
    "mocha": "^10.2.0",
    "nock": "^13.0.4",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "^10.8.2"
  },
  "gitHead": "d9968fc5de73b58186c283034ed2b7b63d81f010"
}
